<Page x:Class="ProteoformExplorer.ProteoformExplorerGUI.ProteoformFamilyVisualization"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProteoformExplorer.ProteoformExplorerGUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ProteoformFamilyVisualization">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="34"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <!--Navigation buttons-->
        <Button x:Name="backToDashboardButton" Grid.Row="0" Content="&lt;" FontWeight="Bold" HorizontalAlignment="Left" 
                Width="30" Height="30" Margin="2" Click="backToDashboardButton_Click" />
        <WpfPlot Name="pfmFamilyVisualizationChart" Grid.Row="1" />
    </Grid>
</Page>
